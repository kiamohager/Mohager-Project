{
    "name": "project-mohager",
    "version": "1.0.0",
    "description": "A Modern Web App Template",
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "build-lambda": "npm run types && cross-env NODE_ENV=production node esbuild.mjs",
        "lint": "eslint \"src/**/*.ts*\"",
        "types": "tsc",
        "bootstrap": "cdk bootstrap",
        "deploy": "cdk deploy --require-approval=never --all",
        "verify-cdk": "cdk synth",
        "check-format": "prettier --check \"src/**/*.ts\" \"infrastructure/**/*.ts\"",
        "format": "prettier --write \"src/**/*.ts\" \"infrastructure/**/*.ts\""
    },
    "author": "Kia Mohager",
    "license": "MIT",
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.788.0",
        "@aws-sdk/lib-dynamodb": "^3.789.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^6.4.8",
        "@mui/material": "^6.4.8",
        "@react-three/fiber": "^9.1.0",
        "@types/spotify-web-playback-sdk": "^0.1.19",
        "aws-cdk-lib": "2.173.2",
        "axios": "^1.8.3",
        "dotenv": "^16.5.0",
        "luxon": "^3.6.1",
        "motion": "^12.6.3",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-router": "^7.4.1",
        "react-spotify-web-playback-sdk": "^3.0.2",
        "spotify-web-api-js": "^1.5.2",
        "three": "^0.174.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.24.0",
        "@types/aws-lambda": "^8.10.149",
        "@types/luxon": "^3.6.2",
        "@types/node": "^22.14.1",
        "@types/react": "^19.0.11",
        "@types/react-dom": "^19.0.4",
        "@types/three": "^0.174.0",
        "@vitejs/plugin-react": "^4.3.4",
        "aws-cdk": "2.173.2",
        "aws-lambda": "^1.0.7",
        "cross-env": "^7.0.3",
        "eslint": "^9.24.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.0.0",
        "prettier": "^3.5.3",
        "sass": "^1.86.0",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.29.1",
        "vite": "^6.2.2"
    }
}
